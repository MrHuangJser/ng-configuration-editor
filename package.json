    "@angular/router": "~11.0.5",
    "@ant-design/icons-angular": "^11.0.1",
    "@ngrx/component-store": "^10.1.2",
    "@ngrx/store": "^10.0.0",
    "lodash": "^4.17.20",
    "ng-zorro-antd": "^11.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
